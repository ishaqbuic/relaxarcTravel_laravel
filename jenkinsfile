pipeline{
    agent{label 'node1'}
    stages{
        stage('Build'){
            steps{
                sh 'pwd'
                sh 'apt install php8.2-fpm -y'
                sh 'php composer.phar install'
                sh 'composer install'
                sh 'mv .env.example .env'
                sh 'apt install nodejs'
                sh 'npm install'
                sh 'php artisan key:generate'
            }
        }
    }
}

